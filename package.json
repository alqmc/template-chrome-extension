{
  "name": "template-chrome-extension",
  "version": "0.0.1",
  "description": "template-chrome-extension",
  "scripts": {
    "dev": "vite",
    "build:vite": "vite build",
    "build": "gulp --require sucrase/register/ts -f build/gulpfile.ts",
    "clear": "rimraf dist",
    "update:version": "sucrase-node build/utils/version.ts",
    "log": "conventional-changelog -p custom-config  -i CHANGELOG.md -s -n ./changelog-option.js",
    "push": "git push origin master && git push githup master",
    "prepare": "husky install"
  },
  "keywords": [
    "chrome",
    "chrome-extension"
  ],
  "author": "Thezero",
  "license": "ISC",
  "devDependencies": {
    "@alqmc/build-ts": "^0.0.9",
    "@alqmc/build-utils": "^0.0.5",
    "@alqmc/build-vue": "0.0.5",
    "@alqmc/eslint-config": "^0.0.4",
    "@commitlint/cli": "^8.3.5",
    "@commitlint/config-angular": "^8.3.4",
    "@rollup/plugin-replace": "^4.0.0",
    "@types/gulp": "^4.0.9",
    "@types/gulp-autoprefixer": "^0.0.33",
    "@types/node": "^17.0.25",
    "@types/rollup-plugin-css-only": "^3.1.0",
    "@vitejs/plugin-vue": "^2.2.2",
    "@vue/compiler-sfc": "^3.0.7",
    "@vue/tsconfig": "^0.1.3",
    "chrome-types": "^0.1.96",
    "commitizen": "^4.1.2",
    "compare-func": "^2.0.0",
    "conventional-changelog-cli": "^2.0.34",
    "conventional-changelog-custom-config": "^0.3.1",
    "cz-conventional-changelog": "^3.2.0",
    "fast-glob": "^3.2.11",
    "gulp": "^4.0.2",
    "gulp-autoprefixer": "^8.0.0",
    "gulp-clean-css": "^4.3.0",
    "gulp-less": "^5.0.0",
    "husky": "^8.0.1",
    "lint-staged": "^10.5.4",
    "less": "^4.1.2",
    "node-html-parser": "^5.3.3",
    "prettier": "^2.7.1",
    "rimraf": "^3.0.2",
    "rollup": "^2.68.0",
    "rollup-plugin-css-only": "^3.1.0",
    "sucrase": "^3.20.3",
    "tslib": "2.0.0",
    "typescript": "^4.5.5",
    "vite": "^2.8.6"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "dependencies": {
    "vue": "^3.2.37",
    "vue-router": "^4.0.16"
  }
}